package predicates;

import java.awt.Color;

public class Tester {
	public static void main(String [] args) {
		StartsWith str = new StartsWith("s");
		System.out.println(str.accepts("start"));
		System.out.println(str.accepts("banana"));
		
	}
	
	
	
	
	
	
	
	public class IsPositive implements Predicate<Integer> {
	    public boolean accepts(Integer t) {
	      return t > 0;
	   }
	}
}
